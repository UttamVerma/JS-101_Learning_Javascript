//Store the name, school, grade, section, rollno and the marks scored by the student in 3 subjects
//Print the report card of the student (You can make it look nice by using some keyboard symbols )





let name="'*'Uttam Verma'*'',";
let school="***D.A.V. Public Sr. Sec. School***";
let grade="GRADE=:.*8*.,";
let section="Section =: 'C',";
let rollno="Rollno := *'37'*,";
let marks="Science=: __--**62**--__";
let marks1="Maths=: +-_68_-+";
let marks2="Hindi=: ^*71*^";
let Rep="Report Card";
console.log(Rep);


