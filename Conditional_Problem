// If the number is divisible by 3, print a "multiple of 3".

let num = 7;
if (num%3==0){
  console.log("multiple of 3",num);
}
else{
  console.log("not a multiple of 3",num);
}



//If a person is allowed to drive in India print "Apply for a license" or "NA".

let legal_age = 18;
let person_age = 17;
if (person_age >= legal_age){
  console.log("Apply for license");
}
else{
  console.log("NA");
}




//Given 2 numbers a and b print which is greater or "both equal".

let a = 20;
let b = 30;
if (a>b){
  console.log("a is greater than b");
}
else if(b>a){
  console.log("b is greater than a");
}
else{
  console.log("Both are equal");
}




//Given stored username and password and input username and password, Print if the user can login or not.

let original_username = "Goku";
let original_password = "Dragon_Ball_Super";
let entered_username = "Vegeta";
let entered_password = "Tournament_Of_Power";
if(entered_username==original_username){
  if(entered_password==original_password){
    console.log("Your login is Successfull");
  }
  else{
    console.log("Invalid Password Entered");
  }
}
else{
  console.log("Invalid Username Entered");
}